// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model AreaAcademica {
  id       Int      @id @default(autoincrement())
  descricao String
  ativo    Boolean
}

model AreaInteresse {
  id       Int      @id @default(autoincrement())
  descricao String
  ativo    Boolean
}
